{
    "extends": "standard",
    "parserOptions": {
      "sourceType": "module"
    },
    "rules": {
      "indent": [
        "error",
        2,
        {
          "SwitchCase": 1
        }
      ],
      "no-array-constructor": 0,
      "no-trailing-spaces": 0,
      "no-unused-vars": 1,
      "eqeqeq": 0,
      "no-proto": 0
    },
    "globals": {
      "App": true,
      "$": true,
      "wx": true,
      "Page": true,
      "Component": true,
      "getCurrentPages": true
    },
    "env": {
      "browser": true,
      "node": true,
      "worker": true,
      "mocha": true,
      "phantomjs": true,
      "serviceworker": true
    }
}